package lab;

public class Array {
	public static void main(String [] args) {
		int [] array = {10,20,30,40};
		System.out.println(array.length);
		
		for (int i=0;i<array.length;i++){
			System.out.println("the elements are:"+array[i]+"\t");
			}
		for(int num:array) {
			System.out.println(num+"\t");
		}
	}
}
